    },
    "closeChatIconStyle": {
      "stroke": "#a2a1a6",
      "width": "28px",
      "height": "28px",
      "paddingLeft": "5px"
    },
    "notificationIconStyle": {
      "fill": "#a2a1a6",
      "width": "28px",
      "height": "28px"
    },
    "notificationIconDisabledStyle": {
      "fill": "#a2a1a6",
      "width": "28px",
      "height": "28px"
    }

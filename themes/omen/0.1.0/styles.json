{
    "headerStyle": {
      "background": "#1a191c",
      "border": "none",
      "borderRadius": "10px 10px 0px 0px"
    },
    "bodyStyle": {
      "backgroundColor": "#131215",
      "border": "none"
    },
    "chatInputContainerStyle": {
      "border": "none",
      "backgroundColor": "#131215",
      "padding": "15px 15px 20px 15px"
    },
    "chatMessagePromptStyle": {
      "color": "#fafafa",
      "padding": "6px"
    },
    "chatHistoryButtonStyle": {
      "background": "none",
      "color": "#fafafa",
      "borderColor": "#fafafa"
    },
    "chatHistoryButtonHoveredStyle": {
      "background": "#fafafa",
      "color": "#131215"
    },
    "sendButtonStyle": {
      "backgroundColor": "#fafafa",
      "width": "55px",
      "height": "40px",
      "borderRadius": "12px"
    },
    "chatInputAreaFocusedStyle": {
      "color": "#fafafa"
